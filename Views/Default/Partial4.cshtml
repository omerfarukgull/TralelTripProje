@using TralelTripProje.Models.Siniflar
@model List<Blog>


@foreach (var x in Model)
{
    <div class="bot-img">
        <img style="height:80px" src="@x.BlogImage" alt="" />
    </div>
    <div class="bot-desc">
        <p>@x.Aciklama.Substring(0,100) </p>
    </div>
    <div class="clear"></div>
}

